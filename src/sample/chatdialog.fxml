<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane onMouseClicked="#changereceiver" pickOnBounds="false" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChatDialogController">
   <children>
      <Button defaultButton="true" layoutX="534.0" layoutY="318.0" mnemonicParsing="false" onAction="#send_message" text="Send" />
      <TextField fx:id="message" layoutX="33.0" layoutY="308.0" onMousePressed="#changereceiver" prefHeight="46.0" prefWidth="481.0" />
      <TextArea fx:id="ta" editable="false" layoutX="14.0" layoutY="55.0" prefHeight="200.0" prefWidth="294.0" wrapText="true">
         <opaqueInsets>
            <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
         </opaqueInsets></TextArea>
      <TextArea fx:id="myta" editable="false" layoutX="307.0" layoutY="55.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="200.0" prefWidth="287.0" wrapText="true">
         <opaqueInsets>
            <Insets />
         </opaqueInsets></TextArea>
      <Button fx:id="send_file_button" layoutX="43.0" layoutY="271.0" mnemonicParsing="false" onAction="#start_file_window" text="Send a File" />
      <Button fx:id="dirchoose" layoutX="425.0" layoutY="271.0" mnemonicParsing="false" onAction="#direchooser" text="dir choose" />
      <ToggleButton fx:id="togglebutton" layoutX="264.0" layoutY="271.0" mnemonicParsing="false" onAction="#encryption_toggle" text="Encryption" />
   </children>
</AnchorPane>
