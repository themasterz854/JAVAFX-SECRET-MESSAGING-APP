   <children>
      <Button defaultButton="true" layoutX="534.0" layoutY="318.0" mnemonicParsing="false" onAction="#send_message" text="Send" />
      <TextField fx:id="message" layoutX="43.0" layoutY="307.0" prefHeight="46.0" prefWidth="481.0" />
      <TextArea fx:id="TA" layoutX="35.0" layoutY="35.0" prefHeight="77.0" prefWidth="330.0" />
      <Button layoutX="428.0" layoutY="138.0" mnemonicParsing="false" onAction="#receive_message" text="Receive Messages" />
